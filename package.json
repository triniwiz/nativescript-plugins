{
	"name": "plugins",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"setup": "npx rimraf node_modules package-lock.json dist tmp yarn.lock && yarn",
		"postinstall": "husky && npx ts-patch install",
		"start": "nps",
		"add": "nx g @nativescript/plugin-tools:add-package",
		"add-angular": "nx g @nativescript/plugin-tools:add-angular",
		"config": "nx g @nativescript/plugin-tools:config",
		"publish-packages": "nx g @nativescript/plugin-tools:publish",
		"docs:build": "cd docs && vuepress build src",
		"docs:dev": "cd docs && vuepress dev src",
		"start:server-demo": "node dist/apps/server-demo/main",
		"sync-packages-with-demos": "nx g @nativescript/plugin-tools:sync-packages-with-demos",
		"remove-package": "nx g @nativescript/plugin-tools:remove-package",
		"add-demo": "nx g @nativescript/plugin-tools:add-demo",
		"prepare": "husky",
		"focus-supabase": "nx g @nativescript/plugin-tools:focus-packages nativescript-supabase,nativescript-supabase-functions,nativescript-supabase-gotrue,nativescript-supabase-postgrest,nativescript-supabase-realtime,nativescript-supabase-storage",
		"lint-fix-no-errors": "eslintNoErrors() { npx eslint --fix ${@} || true; }; eslintNoErrors"
	},
	"private": true,
	"devDependencies": {
		"@angular-devkit/build-angular": "^18.0.0",
		"@angular/animations": "^18.0.0",
		"@angular/common": "^18.0.0",
		"@angular/compiler": "^18.0.0",
		"@angular/compiler-cli": "^18.0.0",
		"@angular/core": "^18.0.0",
		"@angular/forms": "^18.0.0",
		"@angular/platform-browser": "^18.0.0",
		"@angular/platform-browser-dynamic": "^18.0.0",
		"@angular/router": "^18.0.0",
		"@descope/core-js-sdk": "2.20.4",
		"@nativescript-community/perms": "~2.3.6",
		"@nativescript/angular": "^18.0.0",
		"@nativescript/core": "~8.8.0",
		"@nativescript/plugin-tools": "5.4.3",
		"@nativescript/types": "~8.8.0",
		"@nativescript/webpack": "~5.0.21",
		"@nestjs/common": "^9.0.0",
		"@nestjs/core": "^9.0.0",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@nestjs/websockets": "^9.0.0",
		"@ngtools/webpack": "^18.0.0",
		"@types/jest": "29.5.0",
		"@types/react": "npm:types-react-without-jsx-intrinsics@^16.14.30",
		"@types/socket.io": "^3.0.2",
		"@valor/nativescript-websockets": "^1.0.3",
		"@klippa/nativescript-http": "3.0.4",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"date-fns": "^2.21.3",
		"express": "^4.17.1",
		"husky": "~9.0.0",
		"jest": "28.1.3",
		"lint-staged": "15.2.5",
		"nativescript-videoplayer": "^5.0.1",
		"nativescript-vue": "~2.9.0",
		"nativescript-vue-template-compiler": "~2.9.0",
		"ng-packagr": "^18.0.0",
		"pusher": "^5.0.0",
		"react": "^16.14.0",
		"react-nativescript": "^4.0.0",
		"react-nativescript-navigation": "^4.0.0",
		"rxjs": "~7.8.0",
		"socket.io": "^4.1.1",
		"svelte": "3.24.1",
		"svelte-native": "~1.0.4",
		"ts-jest": "28.0.8",
		"tslib": "^2.0.0",
		"typescript": "~5.5.0",
		"vuepress": "^1.9.8",
		"whatwg-url": "11.0.0",
		"zone.js": "~0.15.0"
	},
	"lint-staged": {
		"**/*.{js,ts,html}": [
			"npm run lint-fix-no-errors --",
			"prettier --write --ignore-unknown"
		],
		"**/*.{css,scss,json}": [
			"prettier --write --ignore-unknown"
		]
	},
	"dependencies": {
		"postcss-import": "^16.1.0"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
